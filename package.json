{
  "name": "forta-agent-starter",
  "version": "0.0.1",
  "description": "Forta Agent Javascript starter project",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec \"forta-agent run --range 13473860..13496415\"",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "abi-decoder": "^2.4.0",
    "bignumber.js": "^9.0.1",
    "ethereum-input-data-decoder": "^0.3.5",
    "forta-agent": "^0.0.24"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^2.0.8"
  }
}
